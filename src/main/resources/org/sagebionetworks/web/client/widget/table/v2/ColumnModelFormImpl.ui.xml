<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

	<g:HTMLPanel>
		<b:Form visible="true">
			<b:FieldSet>
				<b:FormGroup validationState="WARNING">
					<b:FormLabel for="columnName">Column Name</b:FormLabel>
					<b:TextBox text="" ui:field="columnName" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel for="columType">Column Type</b:FormLabel>
					<b:InputGroup ui:field="columnTypeGroup">
						<b:ButtonGroup dataToggle="BUTTONS">
							<b:RadioButton ui:field="stringButton">String</b:RadioButton>
							<b:RadioButton ui:field="integerButton">Integer</b:RadioButton>
							<b:RadioButton ui:field="doubleButton">Double</b:RadioButton>
							<b:RadioButton ui:field="booleanButton">Boolean</b:RadioButton>
							<b:RadioButton ui:field="dateButton">Date</b:RadioButton>
							<b:RadioButton ui:field="fileButton">File</b:RadioButton>
						</b:ButtonGroup>
					</b:InputGroup>
				</b:FormGroup>
			</b:FieldSet>
		</b:Form>
	</g:HTMLPanel>
</ui:UiBinder>