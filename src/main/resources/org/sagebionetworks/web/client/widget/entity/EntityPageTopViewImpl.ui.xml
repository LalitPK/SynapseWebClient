<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.dom.client">

	<ui:with field='icons'
		type='org.sagebionetworks.web.client.IconsImageBundle' />

	<g:HTMLPanel styleName="margin-left-15 margin-right-15">
				<div class="row" style="margin-top: 5px !important">
					<div style="white-space: normal;">
						<g:SimplePanel ui:field="breadcrumbsPanel" styleName="col-md-12"/>
					</div>
				</div>

				<div class="row">
					<g:SimplePanel ui:field="actionMenuPanel" styleName="col-md-12"/>
				</div>
											
				<div class="row">
				    <g:SimplePanel ui:field="topFullWidthPanel" styleName="col-md-12"/>
			    </div>
				    
				<div class="row" ui:field="navtabContainer">
				    <ul class="nav nav-tabs margin-left-15 margin-right-15">
				        <!-- remove span-8 after upgrade to Bootstrap 3.0, use nav-justified on parent ul instead -->
					    <li class="span-8 active" ui:field="wikiListItem"><g:Anchor styleName="text-align-center no-background" ui:field="wikiLink"/></li>
					    <li class="span-8" ui:field="filesListItem"><g:Anchor styleName="text-align-center link no-background" ui:field="fileLink"/></li>
					    <li class="span-8" ui:field="adminListItem"><g:Anchor styleName="text-align-center link no-background" ui:field="adminLink"/></li>
				    </ul>
				</div>
				
				<div class="row">
					<div class="col-md-8"><g:SimplePanel ui:field="colLeftPanel" /></div>
					<div class="col-md-4" style="float:right; !important"><g:SimplePanel ui:field="colRightPanel" /></div>
				</div>
											
				<div class="row">
					<g:SimplePanel ui:field="fullWidthPanel" styleName="col-md-12"/>
				</div>								
	</g:HTMLPanel>
</ui:UiBinder>

