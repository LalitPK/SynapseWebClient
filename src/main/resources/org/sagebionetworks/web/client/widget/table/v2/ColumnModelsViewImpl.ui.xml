<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:t="urn:org.sagebionetworks.web.client.view.bootstrap.table">

	<g:HTMLPanel>
		<b:Panel>
			<b:PanelHeader visible="false" ui:field="panelHeader"></b:PanelHeader>
			<b:PanelBody>
				<t:Table ui:field="table">
					<t:THead>
						<t:TableRow>
							<t:TableHeader>Column Name</t:TableHeader>
							<t:TableHeader>Column Type</t:TableHeader>
						</t:TableRow>
					</t:THead>
					<t:TBody ui:field="tableBody" visible=false></t:TBody>
				</t:Table>
			</b:PanelBody>
			<b:PanelFooter visible="false" ui:field="panelFooter">
				<b:Button>Add Column</b:Button>
			</b:PanelFooter>
		</b:Panel>
	</g:HTMLPanel>

</ui:UiBinder>