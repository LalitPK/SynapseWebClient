<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">

	<g:HTMLPanel>
		<b:Panel>
			<b:PanelHeader visible="false" ui:field="panelHeader">
				<b:ButtonGroup>
					<b:Button ui:field="addColumnButton" icon="PLUS">Add Column</b:Button>
					<b:Button ui:field="upButton" icon="ARROW_UP" enabled="false">Move
						Up</b:Button>
					<b:Button ui:field="downButton" icon="ARROW_DOWN" enabled="false">Move
						Down</b:Button>
					<b:Button ui:field="deleteButton" icon="TIMES" enabled="false">Remove
						Column</b:Button>
				</b:ButtonGroup>
			</b:PanelHeader>
			<b:PanelBody>
				<t:Table ui:field="table" styleName="table table-striped">
					<t:THead>
						<t:TableRow>
							<t:TableHeader width="25px">
								<b:DropDown visible="false" ui:field="selectDropDown">
									<b:Button dataToggle="DROPDOWN" icon="SQUARE_O"></b:Button>
									<b:DropDownMenu>
										<b:DropDownHeader>Select</b:DropDownHeader>
										<b:AnchorListItem>All</b:AnchorListItem>
										<b:AnchorListItem>None</b:AnchorListItem>
									</b:DropDownMenu>
								</b:DropDown>
							</t:TableHeader>
							<t:TableHeader width="150px">
								<b.html:Strong>Column Name</b.html:Strong>
							</t:TableHeader>
							<t:TableHeader width="120px">
								<b.html:Strong>Column Type</b.html:Strong>
							</t:TableHeader>
							<t:TableHeader width="75px">
								<b.html:Strong>Size</b.html:Strong>
							</t:TableHeader>
							<t:TableHeader>
								<b.html:Strong>Default Value</b.html:Strong>
							</t:TableHeader>
						</t:TableRow>
					</t:THead>
					<t:TBody ui:field="tableBody" visible="false"></t:TBody>
				</t:Table>
			</b:PanelBody>
			<b:PanelFooter visible="false" ui:field="panelFooter">
			</b:PanelFooter>
		</b:Panel>
	</g:HTMLPanel>

</ui:UiBinder>