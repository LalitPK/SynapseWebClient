<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<ui:with field='sageImageBundle' type='org.sagebionetworks.web.client.SageImageBundle' />

	<g:HTMLPanel>
		<g:SimplePanel ui:field="previewPanel" />
		<g:SimplePanel ui:field="trackerPanel" />
		<b:Row>
			<b:Column size="XS_3" offset="XS_5" ui:field="spinnerColumn"
				visible="false">
				<g:Image resource='{sageImageBundle.loading31}' />
				<bh:Text ui:field="spinnerText"></bh:Text>
			</b:Column>
		</b:Row>
		<b:Button dataToggle="COLLAPSE" dataTarget="#optionsCollapse" text="Options"/>
		<b:Collapse b:id="optionsCollapse">
			<g:SimplePanel ui:field="optionsPanel" />
		</b:Collapse>
	</g:HTMLPanel>

</ui:UiBinder>